<table class="table" >
  <thead>
    <tr>
      <th>Artista</th>
      <th>Inicio</th>
      <th>Fin</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.start %></td>
        <td><%= event.end %></td>
        

      <div id="class"class="btn-group-orizontal">
       <td><button type="button" class="btn btn-default"><%= link_to 'Ver detalle', event %></button></td>
       <td> <button type="button" class="btn btn-primary"><%= link_to 'Editar', edit_event_path(event) %></button></td>
        <td><button type="button" class="btn btn-danger"><%= link_to 'Eliminar', event, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
      </tr>
    <% end %>
  </tbody>
</table>
